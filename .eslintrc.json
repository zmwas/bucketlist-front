{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "overrides": [{
      "files": ["/src/*.js"],
      "excludedFiles": ["*.test.js"]
    }]
  },
  "rules": {
    "semi": 2
  },
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ]
}


